generate 2 mutants
in "data/out/"
from "data/model/"
metamodel "C:/eclipse/runtime-EclipseApplication/wcl3/data/model/UMLClassDiagram.ecore"

with commands {
	ac = select one AssociationClass
	cl = create Class with {
		features = cl->features,
		super = cl->super,
		supplier = cl->supplier
	}
	modify cl with {
		copy (abstract, cl.abstract),
		copy (derived, cl.derived),
		copy (name, cl.name),
		copy (scope, cl.scope),
		copy (visibility, cl.visibility)
	}
	remove ac
}