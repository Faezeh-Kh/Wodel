generate 2 mutants
in "data/out/"
from "data/model/"
metamodel "C:/eclipse/runtime-EclipseApplication/SecurityPoliciesWodelSamplePPR/data/model/SecurityPolicy.ecore"

with commands {
	p1 = select one Parameter
	p2 = select one Parameter where {self <> p1 and type = p1->type}
	r = select one Rule where {parameters = p1}
	remove p1 from r->parameters
	modify r with {parameters = p2}
}